{
  "name": "holista-therapy-ui",
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "engines": {
    "node": "^18.15.0",
    "npm": "^9.5.0"
  },
  "scripts": {
    "localdev": "vite --mode localdev",
    "dev": "vite --mode dev",
    "qa": "vite --mode qa",
    "build:dev": "npm run prebuild && tsc && vite build --mode dev",
    "build:qa": "npm run prebuild && tsc && vite build --mode qa",
    "build:uat": "npm run prebuild && tsc && vite build --mode uat",
    "build:prod": "npm run prebuild && tsc && vite build --mode prod",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@hookform/resolvers": "^3.0.0",
    "@mui/lab": "^5.0.0-alpha.124",
    "@mui/material": "^5.11.13",
    "@mui/x-date-pickers": "^6.18.6",
    "@tanstack/react-query": "^4.28.0",
    "@tanstack/react-query-devtools": "^4.28.0",
    "axios": "^1.3.4",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.3",
    "framer-motion": "^10.10.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-icons": "^4.8.0",
    "react-number-format": "^5.1.4",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "^6.9.0",
    "socket.io-client": "^4.7.2",
    "zod": "^3.21.4",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/dompurify": "^3.0.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jsdom": "^21.1.1",
    "prettier": "^2.8.4",
    "sass": "^1.59.3",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^2.4.0",
    "vite-tsconfig-paths": "^4.0.7",
    "vitest": "^0.29.8"
  }
}
