{
  "name": "holista-therapy-ui",
  "private": true,
  "version": "1.1.2",
  "type": "module",
  "engines": {
    "node": "^20.10.0",
    "npm": "^10.2.5"
  },
  "scripts": {
    "localdev": "vite --mode localdev",
    "dev": "vite --mode dev",
    "qa": "vite --mode qa",
    "build:dev": "npm run prebuild && tsc && vite build --mode dev",
    "build:qa": "npm run prebuild && tsc && vite build --mode qa",
    "build:uat": "npm run prebuild && tsc && vite build --mode uat",
    "build:prod": "npm run prebuild && tsc && vite build --mode prod",
    "preview": "vite preview",
    "test": "vitest",
    "format": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "npm run generate-build-version",
    "generate-build-version": "node generate-build-version.cjs"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.4",
    "@mui/lab": "^5.0.0-alpha.159",
    "@mui/material": "^5.15.3",
    "@mui/x-date-pickers": "^6.18.7",
    "@tanstack/react-query": "^5.17.9",
    "@tanstack/react-query-devtools": "^5.17.9",
    "axios": "^1.6.5",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.8",
    "framer-motion": "^10.17.9",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-icons": "^4.12.0",
    "react-number-format": "^5.3.1",
    "react-otp-input": "^3.1.1",
    "react-router-dom": "^6.21.1",
    "socket.io-client": "^4.7.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/dompurify": "^3.0.5",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.1.3"
  }
}